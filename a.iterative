#include<iostream>
using namespace std;
int main()
{
	int a[100],i,n,num,c=0;
	cout<<"enter the size of array"<<endl;
	cin>>n;
	cout<<"enter the numbers for array."<<endl;
	for(i=0;i<n;i++)
	{
		cin>>a[i];
	}
	cout<<"enter the number to search"<<endl;
	cin>>num;
	for(i=0;i<n;i++)
	{
		if(a[i]==num){
			c=1;
			cout<<"element found at "<<i+1<<endl;
		}
		
	}
	if(c==0){
		cout<<"element not found in list"<<endl;
	}
	return 0;
}



